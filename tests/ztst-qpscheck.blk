#include <lib-common/qps.h>

int main(int argc, const char **argv)
{
    int res = 0;

    MODULE_REQUIRE(qps);

    for (int i = 1; i < argc; i++) {
        if (__qps_check_consistency(argv[i], "ztst"))
            res = e_error("inconsistent qps: %s", argv[i]);
    }

    MODULE_RELEASE(qps);
    return res;
}
