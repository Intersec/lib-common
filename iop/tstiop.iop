struct FullDefVal {
    byte i8 = -1;
    ubyte u8 = 2;
    short i16 = -3;
    ushort u16 = 4;
    int i32 = -5;
    uint u32 = 6;
    long i64 = -7;
    ulong u64 = 8;
    bool b = true;
    TestEnum e = TEST_ENUM_C;
    double d = 3.14;
    string s = "Default value";
    bytes data = "\x42";
